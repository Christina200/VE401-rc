Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {GridBox[{
       {
        RowBox[{
         RowBox[{"ln", " ", "L", 
          RowBox[{"(", 
           RowBox[{"\[Mu]", ",", "\[Sigma]"}], ")"}]}], "=", 
         RowBox[{
          RowBox[{
           UnderoverscriptBox["\[Sum]", 
            RowBox[{"i", "=", "1"}], "n"], 
           RowBox[{"ln", " ", 
            RowBox[{"(", 
             RowBox[{
              FractionBox["1", 
               RowBox[{
                SqrtBox[
                 RowBox[{"2", "\[Pi]"}]], "\[Sigma]"}]], 
              SuperscriptBox["e", 
               RowBox[{
                RowBox[{"-", 
                 FractionBox["1", "2"]}], 
                SuperscriptBox[
                 RowBox[{"(", 
                  FractionBox[
                   RowBox[{
                    SubscriptBox["x", "i"], "-", "\[Mu]"}], "\[Sigma]"], 
                  ")"}], "2"]}]]}], ")"}]}]}], "=", 
          RowBox[{
           RowBox[{
            UnderoverscriptBox["\[Sum]", 
             RowBox[{"i", "=", "1"}], "n"], 
            RowBox[{"[", 
             RowBox[{
              RowBox[{"ln", " ", 
               RowBox[{"(", 
                FractionBox["1", 
                 RowBox[{
                  SqrtBox[
                   RowBox[{"2", "\[Pi]"}]], "\[Sigma]"}]], ")"}]}], "+", 
              RowBox[{
               RowBox[{"-", 
                FractionBox["1", "2"]}], 
               SuperscriptBox[
                RowBox[{"(", 
                 FractionBox[
                  RowBox[{
                   SubscriptBox["x", "i"], "-", "\[Mu]"}], "\[Sigma]"], ")"}],
                 "2"]}]}], "]"}]}], "=", 
           RowBox[{
            RowBox[{"n", " ", "ln", " ", 
             RowBox[{"(", 
              FractionBox["1", 
               RowBox[{
                SqrtBox[
                 RowBox[{"2", "\[Pi]"}]], "\[Sigma]"}]], ")"}]}], "-", 
            RowBox[{
             FractionBox["1", 
              RowBox[{"2", 
               SuperscriptBox["\[Sigma]", "2"]}]], 
             RowBox[{
              UnderoverscriptBox["\[Sum]", 
               RowBox[{"i", "=", "1"}], "n"], 
              SuperscriptBox[
               RowBox[{"(", 
                RowBox[{
                 SubscriptBox["x", "i"], "-", "\[Mu]"}], ")"}], 
               "2"]}]}]}]}]}]}]}
      },
      GridBoxAlignment->{"Columns" -> {{"="}}}]}
   },
   GridBoxItemSize->{"Columns" -> {{
        Scaled[0.96]}}}], 
  TraditionalForm]],ExpressionUUID->"1fddd901-fed3-492a-a6f2-ef66b5a46416"]], \
