Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {
     RowBox[{GridBox[{
        {
         RowBox[{
          FractionBox[
           RowBox[{
            RowBox[{"\[PartialD]", "ln"}], " ", 
            RowBox[{"L", "(", 
             RowBox[{"\[Mu]", ",", "\[Sigma]"}], ")"}]}], 
           RowBox[{"\[PartialD]", "\[Mu]"}]], "=", 
          RowBox[{
           FractionBox["1", 
            RowBox[{"2", 
             SuperscriptBox["\[Sigma]", "2"]}]], 
           RowBox[{
            UnderoverscriptBox["\[Sum]", 
             RowBox[{"i", "=", "1"}], "n"], 
            RowBox[{"2", 
             RowBox[{"(", 
              RowBox[{
               SubscriptBox["x", "i"], "-", "\[Mu]"}], ")"}]}]}]}]}]}
       },
       GridBoxAlignment->{"Columns" -> {{"="}}}], "=", 
      RowBox[{
       RowBox[{"0", "\[Implies]", 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"i", "=", "1"}], "n"], 
         SubscriptBox["x", "i"]}]}], "=", 
       RowBox[{
        RowBox[{
         RowBox[{"n", " ", "\[Mu]"}], "\[Implies]", 
         OverscriptBox["\[Mu]", "^"]}], "=", 
        RowBox[{
         RowBox[{
          FractionBox["1", "n"], 
          RowBox[{
           UnderoverscriptBox["\[Sum]", 
            RowBox[{"i", "=", "1"}], "n"], 
           SubscriptBox["x", "i"]}]}], "=", 
         OverscriptBox["X", "_"]}]}]}]}]}
   },
   GridBoxItemSize->{"Columns" -> {{
        Scaled[0.96]}}}], 
  TraditionalForm]],ExpressionUUID->"75feee55-00d1-48a3-a8b0-1c54b098dcbc"]], \
