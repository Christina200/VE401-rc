Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {GridBox[{
       {
        RowBox[{GridBox[{
           {
            RowBox[{
             FractionBox[
              RowBox[{
               RowBox[{"\[PartialD]", "ln"}], " ", 
               RowBox[{"L", "(", 
                RowBox[{"\[Mu]", ",", "\[Sigma]"}], ")"}]}], 
              RowBox[{"\[PartialD]", "\[Sigma]"}]], "=", 
             RowBox[{
              RowBox[{"-", 
               FractionBox["n", "\[Sigma]"]}], "+", 
              RowBox[{
               FractionBox["1", 
                SuperscriptBox["\[Sigma]", "3"]], 
               RowBox[{
                UnderoverscriptBox["\[Sum]", 
                 RowBox[{"i", "=", "1"}], "n"], 
                SuperscriptBox[
                 RowBox[{"(", 
                  RowBox[{
                   SubscriptBox["x", "i"], "-", "\[Mu]"}], ")"}], "2"]}]}]}]}]}
          },
          GridBoxAlignment->{"Columns" -> {{"="}}}], "=", 
         RowBox[{
          RowBox[{"0", "\[Implies]", 
           RowBox[{"n", " ", 
            SuperscriptBox["\[Sigma]", "2"]}]}], "=", 
          RowBox[{
           RowBox[{
            RowBox[{
             UnderoverscriptBox["\[Sum]", 
              RowBox[{"i", "=", "1"}], "n"], 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{
                SubscriptBox["x", "i"], "-", "\[Mu]"}], ")"}], "2"]}], 
            "\[Implies]", 
            SuperscriptBox[
             OverscriptBox["\[Sigma]", "^"], "2"]}], "=", 
           RowBox[{
            FractionBox["1", "n"], 
            RowBox[{
             UnderoverscriptBox["\[Sum]", 
              RowBox[{"i", "=", "1"}], "n"], 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{
                SubscriptBox["x", "i"], "-", "\[Mu]"}], ")"}], 
              "2"]}]}]}]}]}]}
      },
