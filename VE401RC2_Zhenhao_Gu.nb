Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {GridBox[{
       {
        RowBox[{
         RowBox[{
          SubscriptBox["f", "X"], "(", "x", ")"}], "=", 
         RowBox[{
          SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
          RowBox[{
           RowBox[{
            SubscriptBox["f", 
             RowBox[{"X", "|", "t"}]], "(", "x", ")"}], 
           RowBox[{
            SubscriptBox["f", "T"], "(", "t", ")"}], 
           RowBox[{"\[DifferentialD]", "t"}]}]}]}]},
       {
        RowBox[{"=", 
         RowBox[{
          SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
          RowBox[{
           FractionBox[
            SuperscriptBox[
             RowBox[{
              SuperscriptBox["e", 
               RowBox[{
                RowBox[{"-", "\[Lambda]"}], " ", "t"}]], "(", 
              RowBox[{"\[Lambda]", " ", "t"}], ")"}], "x"], 
            RowBox[{"\[CapitalGamma]", "(", 
             RowBox[{"x", "+", "1"}], ")"}]], 
           FractionBox[
            SuperscriptBox["\[Mu]", "k"], 
            TemplateBox[{"k"},
             "Gamma"]], " ", 
           SuperscriptBox["t", 
            RowBox[{"k", "-", "1"}]], " ", 
           SuperscriptBox["e", 
            RowBox[{
             RowBox[{"-", "\[Mu]"}], " ", "t"}]], 
           RowBox[{"\[DifferentialD]", "t"}]}]}]}]},
       {
        RowBox[{"=", 
         RowBox[{
          FractionBox[
           RowBox[{
            SuperscriptBox["\[Mu]", "k"], 
            SuperscriptBox["\[Lambda]", "x"]}], 
           RowBox[{
            RowBox[{"\[CapitalGamma]", "(", 
             RowBox[{"x", "+", "1"}], ")"}], 
            TemplateBox[{"k"},
             "Gamma"]}]], 
          RowBox[{
           SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
           RowBox[{
            SuperscriptBox["t", 
             RowBox[{"x", "+", "k", "-", "1"}]], " ", 
            SuperscriptBox["e", 
             RowBox[{
              RowBox[{"-", 
               RowBox[{"(", 
                RowBox[{"\[Mu]", "+", "\[Lambda]"}], ")"}]}], "t"}]], 
            RowBox[{"\[DifferentialD]", "t"}]}]}]}]}]},
       {
        RowBox[{"=", 
         RowBox[{
          FractionBox[
           RowBox[{
            SuperscriptBox["\[Mu]", "k"], 
            SuperscriptBox["\[Lambda]", "x"]}], 
           RowBox[{
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{"\[Mu]", "+", "\[Lambda]"}], ")"}], 
             RowBox[{"x", "+", "k"}]], 
            RowBox[{"\[CapitalGamma]", "(", 
             RowBox[{"x", "+", "1"}], ")"}], 
            TemplateBox[{"k"},
             "Gamma"]}]], 
          RowBox[{
           SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
           RowBox[{
            SuperscriptBox["z", 
             RowBox[{"x", "+", "k", "-", "1"}]], " ", 
            SuperscriptBox["e", 
             RowBox[{"-", "z"}]], 
            RowBox[{"\[DifferentialD]", "z"}], "           ", 
            RowBox[{"(", 
             RowBox[{"z", ":=", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"\[Mu]", "+", "\[Lambda]"}], ")"}], "t"}]}], 
             ")"}]}]}]}]}]},
       {
        RowBox[{"=", 
         RowBox[{
          RowBox[{
           FractionBox[
            RowBox[{"\[CapitalGamma]", "(", 
             RowBox[{"x", "+", "k"}], ")"}], 
            RowBox[{
             RowBox[{"\[CapitalGamma]", "(", 
              RowBox[{"x", "+", "1"}], ")"}], 
             TemplateBox[{"k"},
              "Gamma"]}]], 
           FractionBox[
            RowBox[{
             SuperscriptBox["\[Mu]", "k"], 
             SuperscriptBox["\[Lambda]", "x"]}], 
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{"\[Mu]", "+", "\[Lambda]"}], ")"}], 
             RowBox[{"x", "+", "k"}]]]}], " ", "=", 
          RowBox[{
           RowBox[{"(", GridBox[{
              {
               RowBox[{"k", "+", "x", "-", "1"}]},
              {
               RowBox[{"k", "-", "1"}]}
             }], ")"}], 
           SuperscriptBox[
            RowBox[{"(", 
