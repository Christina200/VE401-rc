Cell[CellGroupData[{

Cell[TextData[{
 StyleBox["Given that the Normal Distribution (\[Mu],\[Sigma]) has MGF ",
  FontFamily->"Latin Modern Sans"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["m", "X"], "(", "t", ")"}], "=", 
    SuperscriptBox["e", 
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["\[Sigma]", "2"], " ", 
        SuperscriptBox["t", "2"]}], "2"], "+", 
      RowBox[{"\[Mu]", " ", "t"}]}]]}], TraditionalForm]],
  FontFamily->"Latin Modern Sans",ExpressionUUID->
  "9c470614-8e10-4f38-b946-a1a0e6062e2d"],
 StyleBox[", use the properties of MGF to prove that ",
  FontFamily->"Latin Modern Sans"],
 Cell[BoxData[
  FormBox[
   RowBox[{"Y", "=", 
    FractionBox[
     RowBox[{"X", "-", "\[Mu]"}], "\[Sigma]"]}], TraditionalForm]],
  FontFamily->"Latin Modern Sans",ExpressionUUID->
  "2010726e-34ce-45f4-976b-b406b4784fcb"],
 StyleBox[" also follows normal distribution.",
  FontFamily->"Latin Modern Sans"]
}], "Subsubsubsection",
 CellChangeTimes->{{3.793186986701313*^9, 3.7931870562898617`*^9}, {
  3.79318709027687*^9, 3.7931870919347763`*^9}, {3.79318731771671*^9, 
  3.7931873181669083`*^9}, {3.793187391812208*^9, 3.793187400445075*^9}, {
  3.7932400070574007`*^9, 
  3.793240017312634*^9}},ExpressionUUID->"c7d5f613-3a6d-42b7-ba25-\
3626993b7842"],

Cell[TextData[{
 "We can easily calculate ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Y", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"X", "-", "\[Mu]"}], "\[Sigma]"], "=", 
     RowBox[{
      RowBox[{
       FractionBox["1", "\[Sigma]"], "X"}], "-", 
      FractionBox["\[Mu]", "\[Sigma]"]}]}]}], TraditionalForm]],
  ExpressionUUID->"f5919168-c9f0-4783-9177-b456176a3cc9"],
 ", we have ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["m", "Y"], "(", "t", ")"}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["e", 
       RowBox[{
        RowBox[{"-", 
         FractionBox["\[Mu]", "\[Sigma]"]}], "t"}]], 
      RowBox[{
       SubscriptBox["m", "X"], "(", 
       RowBox[{
        FractionBox["1", "\[Sigma]"], "t"}], ")"}]}], "=", 
     RowBox[{
      RowBox[{
       SuperscriptBox["e", 
        RowBox[{
         RowBox[{"-", 
          FractionBox["\[Mu]", "\[Sigma]"]}], "t"}]], 
       SuperscriptBox["e", 
        RowBox[{
         FractionBox[
          RowBox[{" ", 
           SuperscriptBox["t", "2"]}], "2"], "+", 
         RowBox[{"\[Mu]", 
          FractionBox["t", "\[Sigma]"]}]}]]}], "=", 
      SuperscriptBox["e", 
       FractionBox[
        RowBox[{" ", 
         SuperscriptBox["t", "2"]}], "2"]]}]}]}], TraditionalForm]],
  ExpressionUUID->"ed027bda-8498-445c-a904-915d35b75f9f"],
 ". We also know that normal random variable with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Mu]", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "9afd04e3-9409-47f6-a9bc-b4f7a74e43c0"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Sigma]", "=", "1"}], TraditionalForm]],ExpressionUUID->
  "59f9ff72-5a1a-4927-b3fa-911da0b10a19"],
 " has MGF ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["m", "Z"], "(", "t", ")"}], "=", 
    SuperscriptBox["e", 
     FractionBox[
      RowBox[{" ", 
       SuperscriptBox["t", "2"]}], "2"]]}], TraditionalForm]],ExpressionUUID->
  "04a63681-adca-495b-aebf-7bada59ed596"],
 ". So ",
 Cell[BoxData[
  FormBox["Y", TraditionalForm]],ExpressionUUID->
  "018a51f4-4995-423a-85a7-56977b3e3bdb"],
 " and ",
 Cell[BoxData[
  FormBox["Z", TraditionalForm]],ExpressionUUID->
  "957d5105-19e3-4e62-a9d4-3fec992a66e6"],
 " has the same distribution (both standard normal)."
}], "Text",
 CellChangeTimes->{{3.793187101817827*^9, 3.7931872257820473`*^9}, {
  3.793187270048196*^9, 
  3.793187364569915*^9}},ExpressionUUID->"65ccede7-7926-4047-90d6-\
973ffab7d507"],

